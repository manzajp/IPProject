<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>relief</servlet-name>
        <servlet-class>_controller.reliefController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>_controller.loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerServlet</servlet-name>
        <servlet-class>_controller.registerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>editProfileServlet</servlet-name>
        <servlet-class>_controller.editProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>viewUsersServlet</servlet-name>
        <servlet-class>_controller.viewUsersServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>usersController</servlet-name>
        <servlet-class>_controller.usersController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>evacController</servlet-name>
        <servlet-class>_controller.evacController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>reportController</servlet-name>
        <servlet-class>_controller.reportController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>dashboardController</servlet-name>
        <servlet-class>_controller.dashboardController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>relief</servlet-name>
        <url-pattern>/relief</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registerServlet</servlet-name>
        <url-pattern>/registerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>editProfileServlet</servlet-name>
        <url-pattern>/editProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>viewUsersServlet</servlet-name>
        <url-pattern>/viewUsersServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>usersController</servlet-name>
        <url-pattern>/usersController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>evacController</servlet-name>
        <url-pattern>/evacController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>reportController</servlet-name>
        <url-pattern>/reportController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>dashboardController</servlet-name>
        <url-pattern>/dashboardController</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
